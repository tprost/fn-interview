{
  "name": "express-example",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "postinstall": "sequelize db:migrate && bower install",
    "start": "node app.js",
    "migrate": "sequelize db:migrate",
    "seed": "node app/models/seed.js",
    "test": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:unit": "npm run test:backend:unit && npm run test:frontend:unit",
    "test:backend:unit": "cross-env NODE_ENV=test mocha app/**/*.spec.js",
    "test:frontend:unit": "karma start --single-run",
    "test:integration": "cross-env NODE_ENV=test mocha tests/integration --opts tests/integration/mocha.opts",
    "test:e2e": "cross-env NODE_ENV=test nightwatch --config config/tests/e2e/nightwatch.conf.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.0",
    "body-parser": "^1.15.2",
    "bower": "^1.8.0",
    "cookie-parser": "^1.4.3",
    "debug": "~2.2.0",
    "express": "^4.14.0",
    "express-nunjucks": "^2.1.2",
    "express-session": "^1.14.2",
    "glob": "^7.1.1",
    "lodash": "^4.17.2",
    "morgan": "^1.7.0",
    "nightwatch": "^0.9.11",
    "nunjucks": "^3.0.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pg": "^6.1.0",
    "sequelize": "^3.23.6",
    "sequelize-cli": "^2.4.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "bluebird": "^3.4.1",
    "browser-sync": "^2.18.5",
    "browser-sync-spa": "^1.0.3",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "chromedriver": "^2.26.1",
    "cross-env": "^3.1.3",
    "expect.js": "^0.3.1",
    "gulp": "^3.9.1",
    "gulp-develop-server": "^0.5.2",
    "gulp-mocha": "^3.0.1",
    "gulp-nightwatch": "^0.3.2",
    "gulp-nodemon": "^2.2.1",
    "gulp-protractor": "^3.0.0",
    "jasmine": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "mocha": "^3.2.0",
    "node-mocks-http": "^1.5.5",
    "phantomjs": "^2.1.7",
    "selenium-server-standalone-jar": "^3.0.1",
    "sinon": "^1.17.7",
    "sqlite3": "^3.1.8",
    "supertest": "^2.0.0",
    "util": "^0.10.3",
    "wdio-dot-reporter": "0.0.6",
    "wdio-mocha-framework": "^0.5.8",
    "webdriverio": "^4.5.2"
  }
}
